{
  "type": "script",
  "seleniumVersion": "2",
  "formatVersion": 1,
  "steps": [
    {
      "type": "get",
      "url": "http://hosted.where2stageit.com/ramtest/nogeoip.html"
    },
	{
	"type": "pause",
	"waitTime": "5000"
	},
    {
      "type": "setElementText",
      "locator": {
        "type": "id",
        "value": "inputaddress"
      },
      "text": "fl"
    },
    {
      "type": "setElementSelected",
      "locator": {
        "type": "xpath",
        "value": "//form[@id='locator_search']/table/tbody/tr[2]/td[2]/select//option[2]"
      }
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "id",
        "value": "search_button"
      }
    },
	{
	"type": "pause",
	"waitTime": "20000"
	}
    
  ]
