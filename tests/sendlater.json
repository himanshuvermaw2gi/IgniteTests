      "text": "12"
    },
    {
      "type": "setElementText",
      "locator": {
        "type": "id",
        "value": "sendMinutes"
      },
      "text": "12"
    },
    {
      "type": "setElementSelected",
      "locator": {
        "type": "xpath",
        "value": "//div[@id='pnlReport']/p[2]/select//option[2]"
      }
