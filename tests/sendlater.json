    },
    {
      "type": "setElementText",
      "locator": {
        "type": "id",
        "value": "messageFromName"
      },
      "text": "kapil"
    },
    {
      "type": "setElementText",
      "locator": {
        "type": "id",
        "value": "messageFrom"
