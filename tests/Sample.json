{
  "type": "script",
  "seleniumVersion": "2",
  "formatVersion": 1,
  "steps": [
    {
      "type": "get",
      "url": "http://hosted.where2stageit.com/ramtest/"
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "id",
        "value": "panel"
      }
    },
    {
      "type": "setElementText",
      "locator": {
        "type": "id",
        "value": "inputaddress"
      },
      "text": "FL"
    },
    {
      "type": "setElementSelected",
      "locator": {
        "type": "xpath",
        "value": "//form[@id='locator_search']/table/tbody/tr[2]/td[2]/select//option[5]"
      }
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "id",
        "value": "search_button"
      }
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "link text",
        "value": "Disney World"
      }
    },
    {
      "type": "pause",
      "waitTime": "9000"
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "xpath",
        "value": "//div[@id='OpenLayers.Feature_98_popup_contentDiv']//div[.='Hours']"
      }
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "xpath",
        "value": "//div[@id='OpenLayers.Feature_98_popup_contentDiv']//div[.='Address']"
      }
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "xpath",
        "value": "//div[@id='OpenLayers.Feature_98_popup_contentDiv']/div[3]/div[2]/div[1]/div[5]/form/input[1]"
      }
    },
    {
      "type": "pause",
      "waitTime": "5000"
    },
    {
      "type": "setElementText",
      "locator": {
        "type": "xpath",
        "value": "//div[@id='OpenLayers.Feature_98_popup_contentDiv']/div[3]/div[2]/div[1]/div[5]/form/input[1]"
      },
      "text": "GA"
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "xpath",
        "value": "//div[@id='OpenLayers.Feature_98_popup_contentDiv']/div[3]/div[2]/div[1]/div[5]/form/input[2]"
      }
    },
    {
      "type": "pause",
      "waitTime": "15000"
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "css selector",
        "value": "#OL_Icon_144 > div"
      }
    },
    {
      "type": "pause",
      "waitTime": "10000"
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "css selector",
        "value": "#OL_Icon_4521 > div"
      }
    }
  ]
}
