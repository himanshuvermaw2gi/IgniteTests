{
  "type": "script",
  "seleniumVersion": "2",
  "formatVersion": 1,
  "steps": [
    {
      "type": "get",
      "url": "http://mobile.where2stageit.com/ramtest/"
    },
    {
      "type": "setElementText",
      "locator": {
        "type": "id",
        "value": "addressline"
      },
      "text": "karol bagh"
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "css selector",
        "value": "input[type=\"submit\"]"
      }
    },

	{
	"type": "pause",
	"waitTime": "4000"
	},
    {
      "type": "clickElement",
      "locator": {
        "type": "xpath",
        "value": "//*[@id='item']/div/div[1]/span"
      }
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "link text",
        "value": "Get Directions"
      }
    },
	{
      "type": "setElementText",
      "locator": {
        "type": "xpath",
        "value": "//*[@id='startaddress']"
      },
      "text": ""
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "id",
        "value": "search"
      }
    },
	{
	"type": "pause",
	"waitTime": "4000"
	},
    {
      "type": "verifyTextPresent",
      "text": "Please enter an address."
    },
	{
	"type": "pause",
	"waitTime": "4000"
	}
  ]
}
