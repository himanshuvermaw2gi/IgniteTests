{
  "type": "script",
  "seleniumVersion": "2",
  "formatVersion": 1,
  "steps": [
    {
      "type": "get",
      "url": "http://mobile.where2stageit.com/ramtest/"
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "xpath",
        "value": "//html"
      }
    },
    {
      "type": "setElementText",
      "locator": {
        "type": "id",
        "value": "addressline"
      },
      "text": "rajendra nagar"
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "css selector",
        "value": "input[type=\"submit\"]"
      }
    },
	{
	"type": "pause",
	"waitTime": "10000"
	}
	{
	  "type": "verifyTextPresent",
      "text": "Niagara, WI 54151 "
	},
    
  ]
}
