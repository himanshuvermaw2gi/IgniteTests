{
  "type": "script",
  "seleniumVersion": "2",
  "formatVersion": 1,
  "steps": [
    {
      "type": "get",
      "url": "http://mobile.where2stageit.com/ramtest/index-nativemap.html"
    },
 {
        "type": "pause",
        "waitTime": "4000"
        },

    {
      "type": "setElementText",
      "locator": {
        "type": "id",
        "value": "addressline"
      },
      "text": "33901"
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "css selector",
        "value": "input[type=\"submit\"]"
      }
    },
        {
        "type": "pause",
        "waitTime": "7000"
        },
    {
      "type": "clickElement",
      "locator": {
        "type": "css selector",
        "value": "div.actions > span"
      }
    },
    {
        "type": "pause",
        "waitTime": "5000"
        },
    {
      "type": "clickElement",
      "locator": {
        "type": "link text",
        "value": "Get Directions"
      }
    },
    {
        "type": "pause",
        "waitTime": "5000"
        },
    {
      "type": "setElementText",
      "locator": {
        "type": "xpath",
        "value": "//*[@id='startaddress']"
      },
      "text": "xyz00012345"
    },
    {
        "type": "pause",
        "waitTime": "6000"
        },
    {
      "type": "clickElement",
      "locator": {
        "type": "id",
        "value": "search"
      }
    },
        {
        "type": "pause",
        "waitTime": "10000"
        },
    {
      "type": "verifyTextPresent",
      "text": "Unable to geocode the current input address. Please try again."
    },
        {
        "type": "pause",
        "waitTime": "3000"
        }
  ]
}
