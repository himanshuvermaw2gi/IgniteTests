{
  "type": "script",
  "seleniumVersion": "2",
  "formatVersion": 1,
  "steps": [
    {
      "type": "get",
      "url": "http://hosted.where2getit.com/wafflehouse/"
    },
	{
	"type": "pause",
	"waitTime": "4000"
	},
    {
      "type": "setElementText",
      "locator": {
        "type": "css selector",
        "value": "td > input[name=\"addressline\"]"
      },
      "text": "fl"
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "id",
        "value": "search_button"
      }
    },
	{
	"type": "pause",
	"waitTime": "4000"
	},
    {
      "type": "clickElement",
      "locator": {
        "type": "link text",
        "value": "Waffle House #504"
      }
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "link text",
        "value": "Waffle House #650"
      }
    },
	{
	"type": "pause",
	"waitTime": "3000"
	}

   
  ]
}
