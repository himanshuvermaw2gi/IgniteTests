{
  "type": "script",
  "seleniumVersion": "2",
  "formatVersion": 1,
  "steps": [
    {
      "type": "get",
      "url": "http://mobile.where2stageit.com/ramtest/"
    },
    {
      "type": "setElementText",
      "locator": {
        "type": "id",
        "value": "addressline"
      },
      "text": "rajendra nagar"
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "css selector",
        "value": "input[type=\"submit\"]"
      }
    },
	{
	"type": "pause",
	"waitTime": "5000"
	},
   
   
    {
      "type": "clickElement",
      "locator": {
        "type": "xpath",
        "value": "//ul[@id='results']//span[.='  Niagara, VA 24014 ']"
      }
    },
	{
	"type": "pause",
	"waitTime": "5000"
	},
    {
      "type": "verifyTextPresent",
      "text": "No locations were found using your search criteria [ , Niagara, VA 24014 US ]. Please try another input address to search for locations."
    }
  ]
}
