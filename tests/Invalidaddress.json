{
  "type": "script",
  "seleniumVersion": "2",
  "formatVersion": 1,
  "steps": [
    {
      "type": "get",
      "url": "http://mobile.where2stageit.com/ramtest/"
    },
    {
      "type": "setElementText",
      "locator": {
        "type": "id",
        "value": "addressline"
      },
      "text": "rajendra nagar, New Delhi"
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "css selector",
        "value": "input[type=\"submit\"]"
      }
    },
    {
      "type": "verifyTextPresent",
      "text": "No locations were found using your search criteria [ rajendra nagar, New Delhi US ]. Please try another input address to search for locations."
    }
  ]
}
